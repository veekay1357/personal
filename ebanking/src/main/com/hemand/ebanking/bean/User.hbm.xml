<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 4.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-4.0.dtd">
<hibernate-mapping>
	<class name="com.hemand.ebanking.bean.Useraccbean" table="b_acc">  
		<id column="ID" name="id" type="java.lang.Integer">
		<generato class="increment"></generato>
		</id>
		<property column="acc_no" name="accno" type="java.lang.String" />
		<property column="name" name="name" type="string" />
		<property column="gender" name="gen" type="java.lang.String" />
		<property column="dob" name="dob" type="java.lang.String" />
		<property column="doj" name="doj" type="java.lang.String" />
		<property column="address" name="add" type="java.lang.String" />
		<property column="mob" name="mob" type="java.lang.String" />
		<property column="email" name="email" type="java.lang.String" />
		<property column="type" name="acctype" type="java.lang.String" />
		<property column="pin" name="pin" type="java.lang.String" />
		<property column="bal" name="bal" type="java.lang.String" />
	</class>
</hibernate-mapping>
<hibernate-mapping>
	<class name="com.hemand.ebanking.bean.Useraccbean" table="trans">
		<id column="id" name="id" type="java.lang.Integer">
		<generator class="increment"></generator>
		</id>
		<property column="acc" name="accno" type="java.lang.String" />
		<property column="credit" name="bal" type="java.lang.String" />
		<property column="debit" value="0" type="java.lang.String" />
		<property column="bal" name="bal" type="java.lang.String" />
		<property column="date" name="doj" type="java.lang.String" />
	</class>
</hibernate-mapping>
<hibernate-mapping>
	<class name="com.hemand.ebanking.bean.Userebankbean" table="login">
		<id column="id" name="id" type="java.lang.Integer">
		<generator class="increment"></generator>
		</id>
		<property column="name" name="tiuname" type="java.lang.String" />
		<property column="pass" name="tiupass" type="java.lang.String" />
		<property column="type" value="User" type="java.lang.String" />
	</class>
</hibernate-mapping>
<hibernate-mapping>
	<class name="com.hemand.ebanking.bean.Userebankbean" table="user">
		<id column="id" name="id" type="java.lang.Integer">
		<generator class="increment"></generator>
		</id>
		<property column="uname" name="tiuname" type="java.lang.String" />
		<property column="upass" name="tiupass" type="java.lang.String" />
		<property column="account" name="tiaccount" type="java.lang.String" />
		<property column="pin" name="tipin" type="java.lang.String" />
	</class>
</hibernate-mapping>
<hibernate-mapping>
	<class name="com.hemand.ebanking.bean.Empaccbean" table="emp_id">
		<id column="id" name="id" type="java.lang.Integer">
		<generator class="increment"></generator>
		</id>
		<property column="empid" name="eid" type="java.lang.String" />
		<property column="empname" name="enam" type="java.lang.String" />
		<property column="mob" name="emob" type="java.lang.String" />
		<property column="email" name="eem" type="java.lang.String" />
		<property column="username" name="eus" type="java.lang.String" />
		<property column="password" name="epas" type="java.lang.String" />
	</class>
</hibernate-mapping>
<hibernate-mapping>
	<class name="com.hemand.ebanking.bean.Empaccbean" table="login">
		<id column="id" name="id" type="java.lang.Integer">
		<generator class="increment"></generator>
		</id>
		<property column="name" name="enam" type="java.lang.String" />
		<property column="pass" name="epas" type="string" />
		<property column="type" value="Employee" type="java.lang.String" />
	</class>
</hibernate-mapping>